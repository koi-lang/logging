import core::console::print
import core::console::println

import std::logging::log_level

pro klog(content: str, type: LogLevel) {
    call print("[")
	
	if type == INFO {
		call print("Info")
	}
	elf type == DEBUG {
		call print("Debug")
	}
	elf type == CONFIG {
		call print("Config")
	}
	elf type == WARNING {
		call print("Warning")
	}
	elf type == FATAL {
		call print("Fatal")
	}
	
    call print("]: ")
    call println(content)
}